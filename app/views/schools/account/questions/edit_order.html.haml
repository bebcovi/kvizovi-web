- title "Izmjena redoslijeda"

= simple_form_for @quiz, url: order_account_quiz_questions_path(@quiz) do |f|

  %ol.sort.m-bottom
    = render collection: questions, partial: "question_short", as: :question, locals: {f: f}

  = buttons do
    = cancel_button "Odustani", account_quiz_questions_path(@quiz)
    = submit_button "Spremi"
